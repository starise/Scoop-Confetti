{
    "version": "89.0.1",
    "description": "Fork of Firefox focused on privacy, security and freedom (Portable Edition)",
    "homepage": "https://librewolf-community.gitlab.io",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/b6797cdcd0bbd04d02ddbf546b86b4ee/librewolf-89.0.1.en-US.win64.zip",
            "hash": "508c81eb19871e9a9cc3e7d5e5f16a3c7ca39aaa703a400c261929be70b5a8f2"
        }
    },
    "extract_dir": "librewolf",
    "persist": [
        "profile",
        "librewolf.cfg"
    ],
    "bin": [
        [
            "librewolf.exe",
            "librewolf-portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "librewolf.exe",
            "LibreWolf Portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "checkver": {
        "url": "https://gitlab.com/api/v4/projects/13852981/releases",
        "jsonpath": "$[0].assets.links",
        "regex": "(?<GUID>[\\w.]+)/librewolf-(?<version>[\\d.]+).en-US.win64.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/$matchGUID/librewolf-$version.en-US.win64.zip"
            }
        }
    }
}
