{
    "version": "89.0",
    "description": "Fork of Firefox focused on privacy, security and freedom (Portable Edition)",
    "homepage": "https://librewolf-community.gitlab.io",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/be06548a0d9f8fcbc0939099a6cbc599/librewolf-89.0.en-US.win64.zip",
            "hash": "5159113d48eb7d829e514574941e92fca56fa1e2d2a3907c6f18517eb43bf3e2"
        }
    },
    "extract_dir": "librewolf",
    "persist": [
        "profile",
        "librewolf.cfg"
    ],
    "bin": [
        [
            "librewolf.exe",
            "librewolf-portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "librewolf.exe",
            "LibreWolf Portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "checkver": {
        "url": "https://gitlab.com/api/v4/projects/13852981/releases",
        "jsonpath": "$[0].assets.links",
        "regex": "(?<GUID>[\\w.]+)/librewolf-(?<version>[\\d.]+).en-US.win64.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/$matchGUID/librewolf-$version.en-US.win64.zip"
            }
        }
    }
}
