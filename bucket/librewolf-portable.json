{
    "version": "90.0.1",
    "description": "Fork of Firefox focused on privacy, security and freedom (Portable Edition)",
    "homepage": "https://librewolf-community.gitlab.io",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/38755c0cbe4a308059a10a915e342228/librewolf-90.0.1.en-US.win64.zip",
            "hash": "b44a84cfe27748723fb172045b8d23084e85b115af543ac67b526721b4ecbfd8"
        }
    },
    "extract_dir": "librewolf",
    "persist": [
        "profile",
        "librewolf.cfg"
    ],
    "bin": [
        [
            "librewolf.exe",
            "librewolf-portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "librewolf.exe",
            "LibreWolf Portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "checkver": {
        "url": "https://gitlab.com/api/v4/projects/13852981/releases",
        "jsonpath": "$[0].assets.links",
        "regex": "(?<GUID>[\\w.]+)/librewolf-(?<version>[\\d.]+).en-US.win64.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/$matchGUID/librewolf-$version.en-US.win64.zip"
            }
        }
    }
}
