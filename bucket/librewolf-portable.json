        "architecture": {
            "64bit": {
                "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/$matchGUID/librewolf-$version.en-US.win64.zip",
                "extract_dir": "librewolf-$version"
            }
        }
    }
