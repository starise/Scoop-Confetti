{
    "version": "90.0",
    "description": "Fork of Firefox focused on privacy, security and freedom (Portable Edition)",
    "homepage": "https://librewolf-community.gitlab.io",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/20fc2d0a03e5a4e2e7fc67ad1d36d1a6/librewolf-90.0.en-US.win64.zip",
            "hash": "922dffdbd4fe7e29023eb8127951fbb3330b0f3a5915a8d7b2b90bab80b0ad3e"
        }
    },
    "extract_dir": "librewolf",
    "persist": [
        "profile",
        "librewolf.cfg"
    ],
    "bin": [
        [
            "librewolf.exe",
            "librewolf-portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "shortcuts": [
        [
            "librewolf.exe",
            "LibreWolf Portable",
            "-profile \"$dir\\profile\""
        ]
    ],
    "checkver": {
        "url": "https://gitlab.com/api/v4/projects/13852981/releases",
        "jsonpath": "$[0].assets.links",
        "regex": "(?<GUID>[\\w.]+)/librewolf-(?<version>[\\d.]+).en-US.win64.zip"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/$matchGUID/librewolf-$version.en-US.win64.zip"
            }
        }
    }
}
