{
    "version": "93.0",
    "description": "Fork of Firefox focused on privacy, security and freedom (Portable Edition)",
    "homepage": "https://librewolf-community.gitlab.io",
    "license": "MPL-2.0",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/librewolf-community/browser/windows/uploads/ebb47d5538eaece9d11b12b23058e682/librewolf-93.0.en-US.win64.zip",
            "hash": "c5f74b300d40324fa74a3780117cf5c7e4583d2f87a7764bdcfb8833913b1e3d",
            "extract_dir": "librewolf-93.0"
        }
    },
    "persist": [
        "Profiles",
        "LibreWolf/librewolf.cfg"
    ],
    "bin": "librewolf-portable.exe",
    "shortcuts": [
        [
            "librewolf-portable.exe",
            "LibreWolf Portable"
        ]
    ],
    "checkver": {
        "url": "https://gitlab.com/api/v4/projects/13852981/releases",
